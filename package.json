{
  "name": "the-childrens-journey-2",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "watch": {
    "build": {
      "patterns": [
        "app"
      ],
      "extensions": "pug"
    },
    "sass": {
      "patterns": [
        "app"
      ],
      "extensions": "sass"
    },
    "scripts": {
      "patterns": [
        "app"
      ],
      "extensions": "js"
    }
  },
  "scripts": {
    "assets": "cp -a app/assets dist && cp -a app/images dist",
    "scripts": "cp -a app/scripts dist",
    "build": "npm run compilePug && npm run sass && npm run assets && npm run scripts",
    "concurrently": "concurrently",
    "compilePug": "pug app --out dist",
    "pug": "pug",
    "publish": "node .bin/publish.js",
    "sass": "node-sass app/stylesheets/application.sass dist/stylesheets/style.css",
    "server": "node .bin/server.js",
    "start": "concurrently \"npm run watch\" \"npm run server\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-watch"
  },
  "author": "ashton.harris.me@gmail.com",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.2",
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "gh-pages": "^1.1.0",
    "node-sass": "^4.7.2",
    "npm-watch": "^0.3.0",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "^1.0.0-alpha6"
  }
}
